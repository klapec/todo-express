{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "no-param-reassign": 0,
    "indent": [ 2, 2, {
      "SwitchCase": 1
    }],
    "func-names": 0,
    "padded-blocks": 0,
    "comma-dangle": [ 2, "never" ],
    "no-warning-comments": 1,
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-unused-vars": [ 1, {
      "vars": "local",
      "args": "after-used"
    }],
    "quote-props": 0,
    "eqeqeq": [ 2, "smart" ],
    "no-trailing-spaces": 0,
    "no-process-exit": 0
  }
}
